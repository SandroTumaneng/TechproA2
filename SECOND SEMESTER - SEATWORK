public class Main {
	public static void main(String[] args) {
		
		int duterte = 1000000000;
		int pacquiao = 0;
		
		try {
			int result = duterte/pacquiao;
			System.out.println("Result: " + result);
		} catch (ArithmeticException e) {
			System.out.println("Built-in Exception: Cannot be divide by zero");
		}
		
		System.out.println("Program continues running...( :] )...");
	}
}